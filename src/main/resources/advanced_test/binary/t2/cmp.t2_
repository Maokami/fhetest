int main(void) {
  EncInt res_, a_, b_;
  a_ = 10; // encrypted
  b_ = 15; // encrypted

  res_ = a_ == b_;  // 0
  print(res_);      // 4

  res_ += a_ < b_; // 1 -- ctxt comparison
  print(res_);      // 4

  res_ += a_ < b_; // 1 -- ctxt comparison
  print(res_);      // 4

  res_ += a_ < b_; // 1 -- ctxt comparison
  print(res_);      // 4

  res_ += a_ < b_;  // 1 -- ctxt comparison
  print(res_);      // 4
  return 0;
}
